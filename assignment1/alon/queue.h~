
struct q_element
{
  int num;
  struct q_element* next;
  struct proc* proc;
};
 
 
struct queue
{
  struct q_element* head;
  struct q_element* tail;
};
 
struct queue* queue_new(void);
struct queue* queue_free( struct queue* );

struct queue* queue_add_element( struct queue*, const int);
struct queue* queue_remove_element( struct queue*);


 
void queue_print( const struct queue* );
void queue_print_element(const struct q_element* );